  "scripts": {
    "bundle": "webpack --config ./webpack/webpack.bundle.js --PATH_TO_CLEAN='bundle'",
    "bundleStats": "webpack --config ./webpack/webpack.bundle.js --PATH_TO_CLEAN='bundle' --profile --json > webpack/webpack-bundle-stats.json",
    "bundleWatch": "webpack --watch --progress --config ./webpack/webpack.bundle.js --PATH_TO_CLEAN='_bundleFront'",
    "deploy": "webpack --config ./webpack/webpack.deploy.js --PATH_TO_CLEAN='deploy'",
    "deployStats": "webpack --config ./webpack/webpack.deploy.js --PATH_TO_CLEAN='deploy' --profile --json > webpack/webpack-deploy-stats.json",
    "aBundle": "webpack-bundle-analyzer ./webpack/webpack-bundle-stats.json",
    "aDeploy": "webpack-bundle-analyzer ./webpack/webpack-deploy-stats.json",
    "prewebpackDevServer": "webpack --config ./webpack/webpack.bundle.js",
    "webpackDevServer": "webpack-dev-server --hot --config ./webpack/webpack.bundle.js",
    "inspectNode": "node --inspect  ./bundle/server/server.js",
    "nodemon": "nodemon --exec babel-node ./back/index.js --config ./nodemon.json --watch ./back/** | pino-colada",
    "start": "node --optimize_for_size back/server.js $PORT",
    "lint": "eslint --fix --config .eslintrc.used './src/**/*.ts{,?}'",
    "lintImports": "eslint --fix --config .eslintrc.sort.imports './src/**/*.ts{,?}'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ziomecka/fast-coding.git"
  },
  "homepage": "https://fast-coding.herokuapp.com/",
